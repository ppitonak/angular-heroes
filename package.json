{
  "name": "angular-heroes",
  "version": "1.0.0",
  "description": "Simple Angular 2 application",
  "repository": {
    "type": "git",
    "url": "https://github.com/ppitonak/angular-heroes"
  },
  "scripts": {
    "lint": "find app -name '*.ts' | xargs tslint -c tslint.json --force",
    "lite": "lite-server",
    "start": "tsc && concurrently \"npm run tsc:w\" \"npm run lite\" ",
    "test": "tsc && concurrently \"npm run tsc:w\" \"karma start karma.conf.js\" ",
    "test:once": "tsc && karma start karma.conf.js --single-run",
    "tsc": "tsc",
    "tsc:w": "tsc -w"
  },
  "license": "ISC",
  "dependencies": {
    "@angular/common": "2.3.1",
    "@angular/compiler": "2.3.1",
    "@angular/core": "2.3.1",
    "@angular/forms": "2.3.1",
    "@angular/http": "2.3.1",
    "@angular/platform-browser": "2.3.1",
    "@angular/platform-browser-dynamic": "2.3.1",
    "@angular/router": "3.3.1",
    "@angular/upgrade": "2.3.1",
    "angular-in-memory-web-api": "0.2.1",
    "bootstrap": "^3.3.7",
    "core-js": "^2.4.1",
    "reflect-metadata": "^0.1.8",
    "rxjs": "5.0.0-rc.4",
    "systemjs": "0.19.41",
    "zone.js": "^0.7.2"
  },
  "devDependencies": {
    "@types/core-js": "^0.9.34",
    "@types/jasmine": "^2.5.37",
    "@types/node": "^6.0.46",
    "codelyzer": "^1.0.0-beta.4",
    "concurrently": "^3.0.0",
    "jasmine-core": "^2.5.2",
    "karma": "^1.3.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-cli": "^1.0.1",
    "karma-htmlfile-reporter": "^0.3.4",
    "karma-jasmine": "^1.0.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "lite-server": "^2.2.2",
    "tslint": "^3.15.1",
    "typescript": "^2.0.3"
  }
}
